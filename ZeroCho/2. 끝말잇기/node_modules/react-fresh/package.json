{
  "name": "react-fresh",
  "version": "0.1.4",
  "description": "Helpful decorators for React to speed up development",
  "main": "build/",
  "homepage": "https://github.com/chrismpettyjohn/react-fresh",
  "bugs": {
    "url": "https://github.com/chrismpettyjohn/react-fresh/issues"
  },
  "keywords": [
    "react",
    "react-fresh",
    "react decorators",
    "react router",
    "react guard"
  ],
  "license": "MIT",
  "dependencies": {
    "history": "^4.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.0.3",
    "@types/react": "16.9.12",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-15": "^1.4.1",
    "enzyme-adapter-react-16": "^1.15.1",
    "gts": "^1.1.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "redux-zero": "^5.1.1",
    "ts-jest": "^24.2.0",
    "typescript": "~3.7.0"
  },
  "scripts": {
    "check": "gts check && prettier --check 'src/**/*.ts'",
    "fix": "prettier --write 'src/**/*.{ts,tsx}' && gts fix",
    "test": "npm run check && npm run test:alone",
    "test:alone": "tsc && CI=true jest --coverage",
    "deploy": "rm -rf build && tsc -p . && cp README.md ./build/README.md  && cp package.json ./build/package.json && cp package-lock.json ./build/package-lock.json && npm publish build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "clearMocks": true,
    "rootDir": "src",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testRegex": ".*.spec.(ts|tsx)?",
    "transform": {
      ".*.ts": "ts-jest",
      ".*.tsx": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "./src/util.tsx"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
